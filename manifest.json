{
  "name": "emojidex_extension",
  "version": "0.1",
  "manifest_version": 2,

  "description": "super naty time & goto genshin web site",

  "options_page": "options.html",

  "permissions" : [
    "tabs", "http://*/*", "https://*/*"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "emojidex_extension"
  }
}